{
  "transport": {
    "name": "rabbitmq",
    "reconnect_on_error": true
  },

  "rabbitmq": {
    "host": "#RABBITMQ_HOST#",
    "port": 5672,
    "vhost": "#RABBITMQ_VHOST#",
    "user": "#RABBITMQ_USER#",
    "password": "#RABBITMQ_PASSWORD#"
  },

  "redis": {
    "host": "#RADIS_HOST#",
    "port": 6379,
    "reconnect_on_error": true
  },
  
  "api": {
    "host": "#API_HOST#",
    "bind": "0.0.0.0",
    "port": 4567,
    "user": "#API_USER#",
    "password": "#API_PASSWORD#"
  }
}